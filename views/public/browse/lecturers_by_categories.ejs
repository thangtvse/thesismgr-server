<!Doctype html>
<html>
<meta charset="utf-8"/>
<%- include('../../partials/head.ejs', {title: "Lecturers"}) %>
<body>
<div id="wrapper">

    <% include ../../partials/public.navbar.ejs %>

    <span id="number-of-pages" hidden><%= numberOfPages %></span>
    <span id="unit-id" hidden><%= unit.id %></span>
    <!-- Navigation -->
    <div id="page-wrapper">

        <div class="container-fluid">
            <!-- show any messages that come back with authentication -->

            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header"><%= unit.name %></h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>

            <div class="row">
                <!-- show any messages that come back with authentication -->
                <div class="alert alert-danger alert-dismissible fade in" role="alert" id="error-alert"
                        <% if (!(message && message.length > 0)) { %>
                     hidden
                        <% } %>
                >
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <strong>Error!</strong>
                    <p id="error-alert-message" style="white-space:pre-wrap;"><%= message %></p>
                </div>
            </div>

            <div class="row" id="lecturer-list">
            </div>

            <nav aria-label="Page navigation" style="text-align: center;">
                <ul id="pagination" class="pagination"></ul>
            </nav>

        </div>
    </div>
</div>
<!-- /#wrapper -->

<%- include ../../partials/scripts.ejs %>
<script type="text/javascript" src="/dist/js/public.browse.min.js"></script>
</body>
</html>

